
var omg = 100;

var result_of_a_complex_calculation = {
    var first = if omg > 100 then { omg + 1 } else { omg - 1 }; // = 99

    // A nice little side-effect
    omg = omg + first; // = 199

    var second = omg * omg; // = 39601

    omg + second
}; // = 39800

if result_of_a_complex_calculation == 39800 then {
    omg
} else {
    false
}


