name "10x fibonacci(26)"

fun fibonacci(n: Int): Int {
    if n <= 0 then {
        0
    } else {
        if n == 1 then {
            1
        } else {
            fibonacci(n - 1) + fibonacci(n - 2)
        }
    }
}


var i = 0;

while i < 10 do {
    var _x = fibonacci(26);
    i = i + 1;
}

0

