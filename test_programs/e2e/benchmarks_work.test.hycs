name "fibonacci(10)"

fun fibonacci(n: Int): Int {
    if n <= 0 then {
        0
    } else {
        if n == 1 then {
            1
        } else {
            fibonacci(n - 1) + fibonacci(n - 2)
        }
    }
}

print_int(fibonacci(10));
expect 55

