
name "GT"

print_bool(2 > 1);
expect 1

print_bool(1 > 2);
expect 0

---

name "GTE"

print_bool(1 >= 1);
expect 1

print_bool(1 >= 2);
expect 0

---

name "LT"

print_bool(1 < 2);
expect 1

print_bool(2 < 1);
expect 0

---

name "LTE"

print_bool(1 <= 1);
expect 1

print_bool(2 <= 1);
expect 0

---

name "Equals<Int>"

print_bool(123 == 123);
expect 1

print_bool(122 == 123);
expect 0

---

name "Equals<Bool>"

print_bool(true == true);
expect 1

print_bool(true == false);
expect 0

---

name "NotEquals<Int>"

print_bool(122 != 123);
expect 1

print_bool(123 != 123);
expect 0

---

name "NotEquals<Bool>"

print_bool(true != false);
expect 1

print_bool(true != true);
expect 0

---

name "Not"

print_bool(not false);
expect 1

print_bool(not true);
expect 0

---

name "And"

print_bool(true and true);
expect 1

print_bool(false and true);
expect 0

---

name "Or"

print_bool(true or true);
expect 1

print_bool(true or false);
expect 1

print_bool(false or false);
expect 0

---

name "Mul"

print_int(7 * 7);
expect 49

---

name "Div"

print_int(7 / 7);
expect 1

---

name "Mod"

print_int(7 % 6);
expect 1

---

name "Add"

print_int(7 + 7);
expect 14

---

name "Sub"

print_int(7 - 7);
expect 0

print_int(999 - 1);
expect 998

---

name "Mul & UnarySub"

print_int(7 * -7);
expect -49
